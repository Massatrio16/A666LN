slot=$(getprop ro.boot.slot_suffix)


if [ "${slot}" = "_a" ]; then
dd if=/dev/block/mapper/vendor_a of=/sdcard/vendor
dd if=/dev/block/mapper/vendor_dlkm_a of=/sdcard/vendor_dlkm
dd if=/dev/block/mapper/odm_a of=/sdcard/odm
elif [ "${slot}" = "_b" ]; then
dd if=/dev/block/mapper/vendor_b of=/sdcard/vendor
dd if=/dev/block/mapper/vendor_dlkm_b of=/sdcard/vendor_dlkm
dd if=/dev/block/mapper/odm_b of=/sdcard/odm
else
dd if=/dev/block/mapper/vendor of=/sdcard/vendor
dd if=/dev/block/mapper/vendor_dlkm of=/sdcard/vendor_dlkm
dd if=/dev/block/mapper/odm of=/sdcard/odm
fi
